{
  "name": "@sciencelivehub/nanopub-create",
  "version": "0.1.0",
  "description": "JavaScript library for creating nanopublications from templates",
  "main": "dist/nanopub-creator.js",
  "module": "dist/nanopub-creator.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/nanopub-creator.esm.js",
      "require": "./dist/nanopub-creator.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js"
    },
    "./styles": "./dist/creator.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist",
    "build:demo": "vite build --config vite.demo.config.js",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx"
  },
  "keywords": [
    "nanopublication",
    "semantic-web",
    "linked-data",
    "rdf",
    "knowledge-graph",
    "science-live",
    "nanodash"
  ],
  "author": "Science Live Hub",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ScienceLiveHub/nanopub-create.git"
  },
  "bugs": {
    "url": "https://github.com/ScienceLiveHub/nanopub-create/issues"
  },
  "homepage": "https://github.com/ScienceLiveHub/nanopub-create#readme",
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {}
}
